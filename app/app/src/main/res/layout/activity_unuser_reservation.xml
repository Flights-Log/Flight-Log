                            android:layout_height="wrap_content"
                            android:layout_marginVertical="7dp"
                            android:layout_marginStart="10dp"
                            android:layout_weight="1"
                            android:text="탑승 인원"
                            android:textSize="18sp"
                            android:textStyle="bold" />

                        <EditText
